# CharacterCut Backend Environment Configuration

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Stage Configuration
STAGE=dev

# Redis Configuration (optional, for session caching)
REDIS_URL=redis://localhost:6379

# Metrics and Monitoring
METRICS_ENABLED=true

# Performance Settings
MAX_CONCURRENT_PROCESSES=10
PROCESSING_TIMEOUT_SECONDS=30

# Security Settings
MAX_FILE_SIZE_MB=10
RATE_LIMIT_PER_MINUTE=30

# Alternative Library Configuration (Phase 0 research)
ENABLE_FALLBACK_LIBRARIES=true
PRIMARY_MODEL=u2net
FALLBACK_MODELS=u2net_human_seg,silueta,isnet-general-use